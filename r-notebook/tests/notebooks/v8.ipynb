    "# Create a new context\n",
    "library(V8)\n",
    "ctx <- v8()\n",
    "\n",
    "# Evaluate some code\n",
    "ctx$eval(\"var foo = 123\")\n",
    "ctx$eval(\"var bar = 456\")\n",
    "ctx$eval(\"foo+bar\")\n",
    "\n",
    "# Assign / get objects\n",
    "ctx$assign(\"foo\", JS(\"function(x){return x*x}\"))\n",
    "ctx$assign(\"bar\", JS(\"foo(9)\"))\n",
    "ctx$get(\"bar\")"
   ]
  }
